{
  "openai": {
    "id": "openai",
    "name": "OpenAI",
    "npm": "@ai-sdk/openai",
    "doc": "https://platform.openai.com/docs/models",
    "env": ["OPENAI_API_KEY"],
    "models": {
      "gpt-5.2": {
        "name": "GPT-5.2",
        "family": "gpt-5",
        "modalities": {
          "input": ["text"],
          "output": ["text"]
        },
        "limit": {
          "context": 100000,
          "output": 1000
        },
        "cost": {
          "input": 1,
          "output": 2
        }
      }
    }
  },
  "vercel": {
    "id": "vercel",
    "name": "Vercel AI Gateway",
    "npm": "@ai-sdk/gateway",
    "doc": "https://github.com/vercel/ai",
    "env": ["VERCEL_AI_GATEWAY_API_KEY"],
    "models": {
      "openai/gpt-5.2": {
        "name": "GPT-5.2 (via Vercel)",
        "family": "gpt-5",
        "modalities": {
          "input": ["text"],
          "output": ["text"]
        },
        "limit": {
          "context": 100000,
          "output": 1000
        },
        "cost": {
          "input": 1,
          "output": 2
        }
      }
    }
  }
}
